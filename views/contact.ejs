  <div id="landing-header" class="container d-flex align-items-center justify-content-center">  
    <form>
        <div class="form-row">
          <div class="form-group col">
            <input type="text" class="form-control" placeholder="First name">
          </div>
          <div class="form-group col">
            <input type="text" class="form-control" placeholder="Last name">
          </div>
        </div>
        <div class="form-row">
          <div class="form-group col">
            <input type="text" class="form-control" placeholder="Subject">
          </div>
        </div>
        <div class="form-row">
            <div class="form-group col">
                <textarea id="form_message" name="message" class="form-control" placeholder="Message" rows="10" required="required" data-error="Please, leave me a message."></textarea>            
              </div>
        </div> 
        <button type="submit" class="btn btn-primary">Submit</button>

      </form>
  </div>

